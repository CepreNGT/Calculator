<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:padding="20dp"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/panel"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/dark_green"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_c"
        app:layout_constraintLeft_toLeftOf="@id/key_c"
        app:layout_constraintRight_toRightOf="@id/key_three"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/key_c"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/for_c"
        android:text="@string/key_c"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_division"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/key_one"
        app:layout_constraintTop_toBottomOf="@id/panel" />

    <Button
        android:id="@+id/key_one"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_1"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_four"
        app:layout_constraintLeft_toRightOf="@id/key_c"
        app:layout_constraintRight_toLeftOf="@id/key_two"
        app:layout_constraintTop_toBottomOf="@id/panel" />

    <Button
        android:id="@+id/key_two"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_2"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_five"
        app:layout_constraintLeft_toRightOf="@id/key_one"
        app:layout_constraintRight_toLeftOf="@id/key_three"
        app:layout_constraintTop_toBottomOf="@id/panel" />

    <Button
        android:id="@+id/key_three"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_3"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_six"
        app:layout_constraintLeft_toRightOf="@id/key_two"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/panel" />


    <Button
        android:id="@+id/key_division"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_div"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_multiple"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/key_four"
        app:layout_constraintTop_toBottomOf="@id/key_c" />

    <Button
        android:id="@+id/key_four"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_4"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_seven"
        app:layout_constraintLeft_toRightOf="@id/key_division"
        app:layout_constraintRight_toLeftOf="@id/key_five"
        app:layout_constraintTop_toBottomOf="@id/key_one" />

    <Button
        android:id="@+id/key_five"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_5"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_eight"
        app:layout_constraintLeft_toRightOf="@id/key_four"
        app:layout_constraintRight_toLeftOf="@id/key_six"
        app:layout_constraintTop_toBottomOf="@id/key_two" />

    <Button
        android:id="@+id/key_six"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_6"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_nine"
        app:layout_constraintLeft_toRightOf="@id/key_five"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/key_three" />


    <Button
        android:id="@+id/key_multiple"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_mult"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_plus"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/key_seven"
        app:layout_constraintTop_toBottomOf="@id/key_division" />

    <Button
        android:id="@+id/key_seven"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_7"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_minus"
        app:layout_constraintLeft_toRightOf="@id/key_multiple"
        app:layout_constraintRight_toLeftOf="@id/key_eight"
        app:layout_constraintTop_toBottomOf="@id/key_four" />

    <Button
        android:id="@+id/key_eight"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_8"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_zero"
        app:layout_constraintLeft_toRightOf="@id/key_seven"
        app:layout_constraintRight_toLeftOf="@id/key_nine"
        app:layout_constraintTop_toBottomOf="@id/key_five" />

    <Button
        android:id="@+id/key_nine"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_9"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_dot"
        app:layout_constraintLeft_toRightOf="@id/key_eight"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/key_six" />


    <Button
        android:id="@+id/key_plus"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_add"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_equal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/key_minus"
        app:layout_constraintTop_toBottomOf="@id/key_multiple" />

    <Button
        android:id="@+id/key_minus"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_sub"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_equal"
        app:layout_constraintLeft_toRightOf="@id/key_plus"
        app:layout_constraintRight_toLeftOf="@id/key_zero"
        app:layout_constraintTop_toBottomOf="@id/key_seven" />

    <Button
        android:id="@+id/key_zero"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_0"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_equal"
        app:layout_constraintLeft_toRightOf="@id/key_minus"
        app:layout_constraintRight_toLeftOf="@id/key_dot"
        app:layout_constraintTop_toBottomOf="@id/key_eight" />

    <Button
        android:id="@+id/key_dot"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_dot"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/key_equal"
        app:layout_constraintLeft_toRightOf="@id/key_zero"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/key_nine" />

    <Button
        android:id="@+id/key_equal"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:backgroundTint="@color/green"
        android:text="@string/key_equal"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/key_plus"
        app:layout_constraintRight_toRightOf="@id/key_dot"
        app:layout_constraintTop_toBottomOf="@id/key_zero" />

</androidx.constraintlayout.widget.ConstraintLayout>